<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
        <Panel id="p2">
        <Button id="btnShowCity" text="{i18n>showCity}" visible="{jsonConfig>/visibleBtnShowCity}" press="onShowCity"/>
        <Button id="btnHideCity" text="{i18n>hideCity}" visible="{jsonConfig>/visibleBtnHideCity}" press="onHideCity"/>
        <Table id="tableEmployee" items="{odataNorthwind>/Employees}">
        <columns>
        <Column id="col1" visible="{jsonConfig>/visibleID}">
        <header>
        <Label id="label1" text="{i18n>tableId}"/>
        </header>
        </Column>
            <Column id="col2" visible="{jsonConfig>/visibleName}">
        <header>
        <Label id="label2" text="{i18n>tableName}"/>
        </header>
        </Column>
            <Column id="col3" visible="{jsonConfig>/visibleCountry}">
        <header>
        <Label id="label3" text="{i18n>tableCountry}"/>
        </header>
        </Column>
       <Column id="col4" visible="{jsonConfig>/visibleCity}">
        <header>
        <Label id="label4" text="{i18n>tableCity}"/>
        </header>
        </Column>
        <Column id="col5" width="100px">
        <header>
        <Label id="label5" text="{i18n>tableShowDetails}"/>
        </header>
        </Column>
        </columns>
        <items>
        <ColumnListItem id="cli" type="Navigation" press="showEmployee">
        <cells>
            <Label id="lb1" text="{odataNorthwind>EmployeeID}"/>
            <Label id="lb2" text="{odataNorthwind>LastName} , {odataNorthwind>FirstName}"/>
            <Label id="lb3" text="{odataNorthwind>Country}"/>
            <Label id="lb4" text="{odataNorthwind>City}"/>
            <core:Icon id="_IDGenIcon1" src="sap-icon://show"  press="showOrders2"></core:Icon>
            </cells>
        </ColumnListItem>
        </items>
        </Table>
            </Panel>
</core:FragmentDefinition>
