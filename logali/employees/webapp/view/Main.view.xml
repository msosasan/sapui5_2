<mvc:View controllerName="logaligroup.employees.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    >
   <Shell id="shell">
    <App id="app">
        <pages>
          <Page id="_IDGenPage1" showHeader="false">
          <content>
          <f:FlexibleColumnLayout id="fcl" backgroundDesign="Solid" layout="OneColumn">
             <f:beginColumnPages>
             <mvc:XMLView id="masterEmployee" viewName="logaligroup.employee.view.MasterEmployee">
             </mvc:XMLView>
             </f:beginColumnPages>
             <f:midColumnPages>
             <mvc:XMLView id="detailEmployeeView" viewName="logaligroup.employee.view.EmployeeDetails">
             </mvc:XMLView>
             </f:midColumnPages>
          </f:FlexibleColumnLayout>
          </content>
          </Page>
            </pages>
    </App>
    </Shell>

</mvc:View>
